@page "/AddFinalInspection"
@using ManufacturingManager.Core
@using Microsoft.AspNetCore.Components.Server.ProtectedBrowserStorage
@using ManufacturingManager.Web.Components
@inject ProtectedSessionStorage SessionStorage
@inject NavigationManager NavigationManager
@attribute [Authorize]
<PageTitle>Add Final Inspection</PageTitle>
@if (SolarAssemblyConfiguration != null)
{
    <FinalInspectionForm SolarAssemblyConfiguration="@SolarAssemblyConfiguration"></FinalInspectionForm>
}

@code{
    private FTCSolarAssemblyConfiguration SolarAssemblyConfiguration { get; set; }
    protected override async Task OnInitializedAsync()
    {
        var configuration  = await SessionStorage.GetAsync<FTCSolarAssemblyConfiguration>("FTCSolarAssemblyConfiguration");
        SolarAssemblyConfiguration = configuration.Value;
       

    }


}